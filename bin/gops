#!/bin/bash

# Get Snapshots

case=$1

case "${case:=all}" in
    baf)
        cd $ROOT_DIR/cases/$case
        wget -O baf-ops.tar.gz https://uofi.box.com/shared/static/5ohg78iold48bb162a8x0eu0t0wlvk8i.gz
        tar -xvf baf-ops.tar.gz &> /dev/null
        ;;
    all)
        cd $ROOT_DIR/bin
        ./gops baf
        ;;
esac
