#!/bin/bash

# Get Operators

case=$1

case "${case:=all}" in
    baf-l2)
        cd $MOR_DIR/cases/baf/
        wget -nv -O baf-l2-ops.tar.gz https://uofi.box.com/shared/static/g6ub5yqv4v1q7xe2nhtfm7c9kl8o36yp.gz
        tar -xvf baf-l2-ops.tar.gz
        ;;
    baf-h10)
        cd $MOR_DIR/cases/baf/
        wget -nv -O baf-h10-ops.tar.gz https://uofi.box.com/shared/static/d4rxo1h2jy6wnt6uxnsd3d3ex5tdbpr0.gz
        tar -xvf baf-h10-ops.tar.gz
        ;;
    all)
        cd $MOR_DIR/bin
        ./gops baf
        ;;
esac
