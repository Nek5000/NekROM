#!/bin/bash

# Get Snapshots

mkdir $MOR_DIR/data
cd $MOR_DIR/data

case=$1

case "${case:=all}" in
    baf)
        wget -nv -O baf.tar.gz https://uofi.box.com/shared/static/bnoflcpf6i6mmofiy4ufmm8gey6t83k1.gz
        tar -xvf baf.tar.gz &> /dev/null
        ;;
    cyl)
        wget -nv -O cyl.tar.gz https://uofi.box.com/shared/static/insxch1bjvzl4bnzr3lpr1c016i4g75p.gz
        tar -xvf cyg.tar.gz &> /dev/null
        wget -nv -O cyls0.f00001 https://uofi.box.com/shared/static/msk1qxdpcw8e0a1olyah9s82cj45acxe.f00001
        ;;
    all)
        cd $MOR_DIR/bin
        ./gsnaps baf
        ./gsnaps cyl
        ;;
esac
