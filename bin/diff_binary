#! /bin/bash

cp us0rf_unit0.f00001 test1; ptr=$(head -1 test1 | strings); sed -i "s/$ptr//g" test1
cp us0rf_unit0.f00002 test2; ptr=$(head -1 test2 | strings); sed -i "s/$ptr//g" test2
cp us0rf_unit0.f00003 test3; ptr=$(head -1 test3 | strings); sed -i "s/$ptr//g" test3
cp us0rf_unit0.f00004 test4; ptr=$(head -1 test4 | strings); sed -i "s/$ptr//g" test4
cp us0rf_unit0.f00005 test5; ptr=$(head -1 test5 | strings); sed -i "s/$ptr//g" test5
cp us0rf_unit0.f00006 test6; ptr=$(head -1 test6 | strings); sed -i "s/$ptr//g" test6
cp $MOR_DIR/data/rft/chan0.f00001 test7; ptr=$(head -1 test7 | strings); sed -i "s/$ptr//g" test7

if cmp --silent -- test5 test7; then
  echo "files contents are identical"
  echo 0 > ecode
else
  echo "files differ"
  echo 99 > ecode
fi

if cmp --silent -- test6 test7; then
  echo "files contents are identical"
  echo 0 > ecode
else
  echo "files differ"
  echo 99 > ecode
fi
iexit=$(cat ./ecode)
echo $iexit
